{
  "name": "gist-automation",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "cypress:run:all": "rm -rf cypress/reports && npx cypress run && npx mochawesome-merge cypress/reports/*.json -o cypress/reports/merged-report.json && npx marge cypress/reports/merged-report.json -o cypress/reports",
    "cypress:run": "npx cypress run",
    "report:merge": "npx mochawesome-merge cypress/reports/*.json -o cypress/reports/merged-report.json",
    "report:generate": "npx marge cypress/reports/merged-report.json -o cypress/reports",
    "executeAll": "rm -rf cypress/reports && npm run cypress:run && sleep 5 && npm run report:merge && npm run report:generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@faker-js/faker": "^9.3.0",
    "@types/chai": "^4.3.20",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mochawesome": "^6.2.4",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-file-upload": "5.0.8",
    "cypress-multi-reporters": "1.6.4",
    "mocha": "^9.2.2"
  },
  "devDependencies": {
    "@types/cypress-cucumber-preprocessor": "^4.0.5",
    "@types/lodash": "^4.17.14",
    "chai-json-schema": "^1.5.1",
    "cypress": "^10.11.0",
    "lodash": "^4.17.21",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "typescript": "^4.9.5"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "step_definitions": "cypress/step_definitions/"
  }
}
